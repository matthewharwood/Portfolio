{
  "modules": true,
  "plugins": {
    "postcss-preset-env": {
      "stage": 3,
      "features": {
        "color-mod-function": { "unresolved": "warn" },
        "custom-media-queries": {
          "preserve": false
        },
        "custom-properties": {
          "preserve": false
        },
        "nesting-rules": true
      }
    }
  }
}